#!/usr/bin/env python

import path_planner as pa
import sys

import rospy


def main():
    rospy.init_node('path_planner')

    path = sys.path[0]
    path_analyzer = pa.PathAnalysis(path + "/../data/coords.gpx")

    path_analyzer.run_standalone("test_file.gpx")

    while not rospy.is_shutdown():
        pass

if __name__ == "__main__":
    main()
